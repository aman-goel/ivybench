(declare-sort epoch 0)
(declare-sort node 0)

(define-fun .epoch ((S epoch)) epoch (! S :sort 4))
(define-fun .node ((S node)) node (! S :sort 2))

(declare-fun __gt (epoch node) Bool)
(declare-fun __held (node) Bool)
